{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@worldcoin/idkit-core": [
        "./node_modules/@worldcoin/idkit-core/build/index"
      ],
      "react-native-reanimated": [
        "./polyfills/react-native-reanimated"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
